<h1><%= @user.username + "\'s page" %></h1>

<%# if @user.avatar %>
  <img src="<%= @user.avatar.url %>">
<%# end %>



<% if current_user.id == params[:id] %>

  <div class="feed">

    <%= render partial: "layouts/user/feed" %>
  </div>

<% else %>

  <div class="show-page">

    <%= render partial: "layouts/user/show" %>

  </div>

<% end %>