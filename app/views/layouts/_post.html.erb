<ul>
<% @user.posts.each do |post| %>

<li class=".post-options">
  <%= post.title %>
  <%= post.body %>

  <%= render partial: "layouts/deletepost", locals: {post: post} %>

  <%= render partial: "layouts/editpost", locals: {post: post} %>

  <%= link_to "show post", user_post_url(current_user.id, post.id), method: :get%>

  <%= render partial: "layouts/postcomments", locals: {post: post} %>

<li>





<% end %>
